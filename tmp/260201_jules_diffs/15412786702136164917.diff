+import { PLR_RESOURCE_DEFINITIONS } from '@assets/browser-data/plr-definitions';
+
+let validPLRClassNamesCache: Set<string> | null = null;
+
+function getValidPLRClassNames(): Set<string> {
+  if (validPLRClassNamesCache) {
+    return validPLRClassNamesCache;
+  }
+
+  const classes = new Set<string>();
+  for (const def of PLR_RESOURCE_DEFINITIONS) {
+    const className = def.fqn.split('.').pop();
+    if (className) {
+      classes.add(className.toUpperCase());
+    }
+  }
+  validPLRClassNamesCache = classes;
+  return classes;
+}
+
+export function isValidPLRClassName(fqn: string): boolean {
+  const className = fqn.split('.').pop()?.toUpperCase();
+  if (!className) {
+    return false;
+  }
+  return getValidPLRClassNames().has(className);
+}
