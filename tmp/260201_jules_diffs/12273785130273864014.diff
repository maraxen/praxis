+import { test, expect } from '../fixtures/worker-db.fixture';
+import { WelcomePage } from '../page-objects/welcome.page';
+
+test.describe('Command Palette', () => {
+  test('opens with keyboard shortcut', async ({ page }) => {
+    await page.goto('/app');
+    await new WelcomePage(page).handleSplashScreen();
+    await page.keyboard.press('Meta+k');
+    await expect(page.locator('app-command-palette')).toBeVisible();
+  });
+
+  test('searches and executes command', async ({ page }) => {
+    await page.goto('/app/settings');
+    await new WelcomePage(page).handleSplashScreen();
+    await page.keyboard.press('Meta+k');
+    await page.locator('input[placeholder="Type a command or search..."]').fill('home');
+
+    const homeCommand = page.locator('button[mat-list-item]', { hasText: 'Go to Home' });
+    
+    await expect(homeCommand).toBeVisible();
+    await homeCommand.click();
+    
+    await expect(page).toHaveURL(/.*home/);
+  });
+});
