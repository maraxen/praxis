{
    "headers": [
        {
            "source": "**/*",
            "headers": [
                {
                    "key": "Cross-Origin-Opener-Policy",
                    "value": "same-origin"
                },
                {
                    "key": "Cross-Origin-Embedder-Policy",
                    "value": "require-corp"
                },
                {
                    "key": "Cross-Origin-Resource-Policy",
                    "value": "cross-origin"
                }
            ]
        }
    ],
    "rewrites": [
        {
            "source": "app/**",
            "destination": "/index.html"
        },
        {
            "source": "playground",
            "destination": "/index.html"
        },
        {
            "source": "settings/**",
            "destination": "/index.html"
        },
        {
            "source": "docs/**",
            "destination": "/index.html"
        }
    ]
}